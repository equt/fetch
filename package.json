{
  "name": "@equt/fetch",
  "version": "3.4.0",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/equt/fetch",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "fp-ts",
    "fetch",
    "HTTP"
  ],
  "files": [
    "README.md",
    "LICENSE.md",
    "dist"
  ],
  "packageManager": "pnpm@7.1.8",
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "build": "unbuild",
    "lint": "eslint .",
    "spell": "cspell '*.json' README.md '**/*.ts'",
    "format": "prettier --check 'src/*.ts' README.md",
    "test": "jest --coverage",
    "check": "tsc",
    "doc": "docs-ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@equt/eslint-config": "^8.1.0",
    "@equt/prettier-config": "^2.5.1",
    "@equt/tsconfig": "^1.3.1",
    "@types/jest": "^28.1.1",
    "cross-fetch": "^3.1.5",
    "cspell": "^6.1.1",
    "docs-ts": "^0.6.10",
    "eslint": "^8.17.0",
    "fetch-mock-jest": "^1.5.1",
    "fp-ts": "2.12.1",
    "husky": "^8.0.1",
    "io-ts": "^2.2.16",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "pnpm": "^7.1.8",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3",
    "unbuild": "^0.7.4"
  },
  "peerDependencies": {
    "fp-ts": ">=2.11.1",
    "io-ts": ">=2.2.8"
  },
  "peerDependenciesMeta": {
    "io-ts": {
      "optional": true
    }
  },
  "prettier": "@equt/prettier-config",
  "eslintConfig": {
    "extends": "@equt"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "unbuild": {
    "entries": [
      "src/index"
    ],
    "clean": true,
    "declaration": true,
    "rollup": {
      "emitCJS": true
    }
  },
  "cspell": {
    "words": [
      "MPTL",
      "abortable",
      "appy",
      "codecov",
      "combinator",
      "combinators",
      "commitlint",
      "contactlab",
      "contactlabappy",
      "equt",
      "failable",
      "fetchm",
      "lazyable",
      "maperror",
      "miliseconds",
      "orio",
      "pnpm",
      "postpublish",
      "runfetchm",
      "runfetchmflipped",
      "runfetchmflippedp",
      "runfetchmflippedpt",
      "runfetchmp",
      "runfetchmpt",
      "tsbuildinfo",
      "tupled",
      "typecheck",
      "withjson",
      "withjsonbody"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
