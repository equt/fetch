{
  "name": "@equt/fetch",
  "version": "2.11.3",
  "license": "MIT",
  "main": "./dist/umd/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    "utils": {
      "import": "./dist/es/utils.js",
      "require": "./dist/cjs/utils.js"
    },
    "combinators": {
      "import": "./dist/es/combinators.js",
      "require": "./dist/cjs/combinators.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "fp-ts",
    "fetch",
    "HTTP"
  ],
  "files": [
    "README.md",
    "LICENSE.md",
    "dist"
  ],
  "scripts": {
    "build": "tsc --outDir dist/es -m ESNext && tsc -m CommonJS --outDir dist/cjs",
    "lint": "eslint .",
    "spell": "cspell '*.json' README.md '**/*.ts'",
    "format": "prettier --check '**/*.ts' README.md",
    "test": "jest --coverage",
    "check": "tsc --noEmit"
  },
  "devDependencies": {
    "@equt/eslint-config": "^8.0.5",
    "@equt/prettier-config": "^2.5.1",
    "@equt/tsconfig": "^1.3.1",
    "@types/jest": "^27.4.0",
    "cross-fetch": "^3.1.5",
    "cspell": "^5.18.5",
    "eslint": "^8.9.0",
    "fetch-mock-jest": "^1.5.1",
    "fp-ts": "2.11.5",
    "io-ts": "^2.2.16",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "fp-ts": ">=2.11.1",
    "io-ts": ">= 2.2.8"
  },
  "peerDependenciesMeta": {
    "io-ts": {
      "optional": true
    }
  },
  "prettier": "@equt/prettier-config",
  "eslintConfig": {
    "extends": "@equt"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "cspell": {
    "words": [
      "abortable",
      "appy",
      "codecov",
      "combinator",
      "combinators",
      "contactlab",
      "contactlabappy",
      "equt",
      "miliseconds",
      "orio",
      "pnpm",
      "postpublish",
      "tsbuildinfo",
      "tupled",
      "typecheck"
    ]
  }
}
