{
  "name": "@equt/fetch",
  "version": "4.4.0",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/equt/fetch",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "fp-ts",
    "fetch",
    "HTTP"
  ],
  "files": [
    "README.md",
    "LICENSE.md",
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "eslint .",
    "format": "prettier --check 'src/*.ts' README.md",
    "test": "jest --coverage",
    "check": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@equt/eslint-config": "^9.0.1",
    "@equt/prettier-config": "^2.5.1",
    "@equt/tsconfig": "^1.3.1",
    "@types/jest": "^29.5.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "io-ts": "^2.2.20",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "io-ts": ">=2.2.8",
    "zod": "^3.21.4"
  },
  "peerDependenciesMeta": {
    "io-ts": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "prettier": "@equt/prettier-config",
  "eslintConfig": {
    "extends": "@equt"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage"
  ],
  "jest": {
    "preset": "ts-jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "fp-ts": "^2.13.1"
  }
}
